// Consider adding the 1918 zones here, if they are not used in your
// organization
//include "/etc/bind/zones.rfc1918";

zone "empresa.local" {
    type master;
    file "/etc/bind/db.empresa.local";
    allow-transfer { 192.168.56.21; };
};

zone "56.168.192.in-addr.arpa" {
    type master;
    file "/etc/bind/db.56.168.192";
    allow-transfer { 192.168.56.21; };
};

zone "jorgecortes.com" {
    type master;
    file "/etc/bind/db.jorgecortes.com";
};


